                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT        inmate.inmate_sys_id AS `ID#`, inmate.f_name AS First, inmate.m_name AS Middle, inmate.l_name AS Last, inmate.full_name AS `Full Name`, 
                         inmate.admiss_dte AS `Admit Dte`, inmate.dob AS DOB, inmate.age AS Age, inmate.o_age AS `P Time`, inmate.sex AS Sex, ethnicity.par_desc AS Ethnic, 
                         inmate.attended_program AS Programs, location_parent.tango AS Location, location.location_name AS Section, inmate.country_id, inmate.i_photo, 
                         inmate_status.par_desc AS `I-Status`, custody_status.par_desc AS `C-Status`
FROM            inmate LEFT OUTER JOIN
                         param custody_status ON inmate.custody_status_id = custody_status.par_num LEFT OUTER JOIN
                         param inmate_status ON inmate.inmate_status_id = inmate_status.par_num LEFT OUTER JOIN
                         location ON inmate.location_id = location.location_id LEFT OUTER JOIN
                         location_parent ON inmate.location_parent_id = location_parent.location_parent_id LEFT OUTER JOIN
                         param ethnicity ON inmate.ethnicity_id = ethnicity.par_num
WHERE        (ethnicity.par_code = 4) AND (custody_status.par_code = 6) AND (inmate_status.par_code = 5)</CommandText>
